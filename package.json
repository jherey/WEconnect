{
  "name": "weconnect",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test npm run migrate && NODE_ENV=test nyc mocha --exit --timeout 25000 ./server/test/**/*.js --compilers js:babel-core/register",
    "babel-node": "babel-node index --presets=env",
    "start-dev": "nodemon --watch server --exec babel-node index.js",
    "start": "babel-node index",
    "production-db-migrate": "sequelize db:migrate:undo:all --env production && sequelize db:migrate --env production",
    "unmigrate": "node ./node_modules/sequelize-cli/lib/sequelize db:migrate:undo:all --env test",
    "migrate": "npm run unmigrate && node ./node_modules/sequelize-cli/lib/sequelize db:migrate --env test",
    "build": "webpack --progress --config ./webpack.prod.js",
    "heroku-postbuild": "NODE_ENV=production && npm run build",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "test:client": "jest",
    "e2e-setup": "selenium-standalone install",
    "start:e2e-test": "NODE_ENV=test nodemon index.js --exec babel-node",
    "e2e:test": "NODE_ENV=test npm run migrate && ./node_modules/.bin/nightwatch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jherey/WeConnect.git"
  },
  "author": "Jeremiah Olufayo",
  "license": "ISC",
